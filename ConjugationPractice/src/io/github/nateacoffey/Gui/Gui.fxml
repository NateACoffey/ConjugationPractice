<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.nateacoffey.Gui.GuiController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <TextArea fx:id="outputTextArea" editable="false" focusTraversable="false" layoutX="38.0" layoutY="93.0" prefHeight="264.0" prefWidth="727.0" text="Using the given verb ending and tense, type the correct conjugation.&#10;">
               <font>
                  <Font size="20.0" />
               </font></TextArea>
            <Button fx:id="helpButton" focusTraversable="false" layoutX="716.0" layoutY="14.0" mnemonicParsing="false" onAction="#showTooltip" text="Help" />
            <TextField fx:id="inputTextBox" layoutX="270.0" layoutY="379.0" onAction="#onEnter" prefHeight="43.0" prefWidth="262.0" text="test">
               <font>
                  <Font size="26.0" />
               </font></TextField>
      </children>
    </AnchorPane>
  </children>
</VBox>
